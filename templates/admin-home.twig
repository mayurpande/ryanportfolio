{% extends 'main.twig' %}
{% set active_page = 'admin' %}


{% block title %}Admin - Update Content{% endblock %}


{% block heading %}Add to home page{% endblock %}

{% block admin %}

	<div class="row">

		<div class=".col-md-6 col-md-offset-3 for">

			<div class="panel panel-default">

				<div class="panel-heading">Home Page</div>

				<div class="panel-body">

					<form action="{{ path_for('adminHome.create') }}" method="post" autcomplete="off">

						<div class="form-group{{ errors.email ? ' has-error' : '' }}">

							<label for="home_img">Home Page Img Url</label>

							<input type"text" name="home_img" id="home_img" class="form-control" >


							<label for="next_ul">Next Gallery</label>

							<input type"text" name="next_ul" id="next_ul" class="form-control" >


						</div>

						<button type="submit" class="btn btn-default">Add home page img url</button>

						{{ csrf.field | raw }}

					</form>

				</div>


            </div>

		</div>

	</div>


{% endblock %}
