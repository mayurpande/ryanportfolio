{% extends 'main.twig' %}
{% set active_page = 'admin' %}


{% block title %}Admin - Update Content{% endblock %}


{% block heading %}Add to landscap page{% endblock %}

{% block admin %}

	<div class="row">

		<div class=".col-md-6 col-md-offset-3 for">

			<div class="panel panel-default">

				<div class="panel-heading">Add to Landscape Page</div>

				<div class="panel-body">

					<form action="{{ path_for('adminLandscape.create') }}" method="post" autcomplete="off">

						<div class="form-group{{ errors.email ? ' has-error' : '' }}">

							<label for="land_img">Landscape Page Img Url</label>

							<input type"text" name="land_img" id="land_img" class="form-control" >

                            <label for="land_light_text">Landscape Page Light Box Text</label>

							<input type"text" name="land_light_text" id="land_light_text" class="form-control" >


						</div>


						<button type="submit" class="btn btn-default">Add landscape page item</button>

						{{ csrf.field | raw }}

					</form>

				</div>


            </div>

		</div>

	</div>


{% endblock %}
