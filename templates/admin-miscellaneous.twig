{% extends 'main.twig' %}
{% set active_page = 'admin' %}

{% block title %}Admin - Update Content{% endblock %}


{% block heading %}Add to miscellaneous page{% endblock %}

{% block admin %}

	<div class="row">

		<div class=".col-md-6 col-md-offset-3 for">

			<div class="panel panel-default">

				<div class="panel-heading">Add to miscellaneous Page</div>

				<div class="panel-body">

					<form action="{{ path_for('adminMiscellaneous.create') }}" method="post" autcomplete="off">

						<div class="form-group{{ errors.email ? ' has-error' : '' }}">

							<label for="misc_img">Miscellaneous Page Img Url</label>

							<input type"text" name="misc_img" id="misc_img" class="form-control" >

                            <label for="misc_light_text">Miscellaneous Light Box text</label>

							<input type"text" name="misc_light_text" id="misc_light_text" class="form-control" >


						</div>


						<button type="submit" class="btn btn-default">Add miscellaneous page</button>

						{{ csrf.field | raw }}

					</form>

				</div>


            </div>

		</div>

	</div>


{% endblock %}
