{% extends 'main.twig' %}
{% set active_page = 'admin' %}

{% block title %}Admin - Update Content{% endblock %}


{% block heading %}Add to portrait page{% endblock %}

{% block admin %}

	<div class="row">

		<div class=".col-md-6 col-md-offset-3 for">

			<div class="panel panel-default">

				<div class="panel-heading">Add to Portrait Page</div>

				<div class="panel-body">

					<form action="{{ path_for('adminPortrait.create') }}" method="post" autcomplete="off">

					<div class="form-group{{ errors.email ? ' has-error' : '' }}">

						<label for="home_img">Gallery Img Url</label>

						<input type"text" name="home_img" id="home_img" class="form-control" >


					</div>

					<div class="form-group{{ errors.email ? ' has-error' : '' }}">

						<label for="ul_id">Gallery Name</label>

						<input type"text" name="ul_id" id="ul_id" class="form-control" >


					</div>



						<button type="submit" class="btn btn-default">Add Gallery</button>

						{{ csrf.field | raw }}

					</form>

				</div>


            </div>

		</div>

	</div>


{% endblock %}
