{% extends 'main.twig' %}
{% set active_page = 'admin' %}

{% block title %}Admin - Update Content{% endblock %}


{% block heading %}Create Landing Image{% endblock %}

{% block admin %}

	<div class="row">

		<div class=".col-md-6 col-md-offset-3 for">

			<div class="panel panel-default">

				<div class="panel-heading">Create Landing</div>

				<div class="panel-body">

					<form action="{{ path_for('adminLand.create') }}" method="post" autcomplete="off">

						<div class="form-group{{ errors.email ? ' has-error' : '' }}">

							<label for="landing_img">Create landing image</label>

							<input type="text" name="landing_img" id="landing_img" class="form-control"/>


						</div>


						<button type="submit" class="btn btn-default">Create Landing</button>

						{{ csrf.field | raw }}

					</form>

				</div>


        </div>

		</div>

	</div>


{% endblock %}
